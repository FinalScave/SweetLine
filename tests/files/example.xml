<?xml version="1.0" encoding="UTF-8"?>
<!-- XML sample -->
<!DOCTYPE catalog SYSTEM "catalog.dtd">

<catalog xmlns="http://example.com/catalog"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://example.com/catalog catalog.xsd">

    <!-- book list -->
    <book id="001" category="fiction">
        <title lang="zh">Three Body</title>
        <author>Liu Cixin</author>
        <year>2008</year>
        <price currency="CNY">36.00</price>
        <description>a science fiction novel</description>
    </book>

    <book id="002" category="tech">
        <title lang="en">The Rust Programming Language</title>
        <author>Steve Klabnik</author>
        <year>2019</year>
        <price currency="USD">39.99</price>
        <available>true</available>
    </book>

    <!-- CDATA section -->
    <script>
        <![CDATA[
            function test() {
                return x < 10 && y > 20;
            }
        ]]>
    </script>

    <!-- entity reference -->
    <note>use &lt;tag&gt; and &amp; symbol</note>
    <special>&#169; copyright &#x2764; love</special>

    <!-- self-closing tag -->
    <meta name="version" value="1.0" />
    <separator />

    <!-- nested structure -->
    <library>
        <section name="Science">
            <shelf row="1" col="A">
                <item ref="001" />
                <item ref="002" />
            </shelf>
        </section>
    </library>
</catalog>
