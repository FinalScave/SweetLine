{
  "name": "markdown",
  "fileExtensions": [".md", ".markdown", ".mdown", ".mkd"],
  "variables": {
    "any": "[\\S\\s]",
    "whiteSpace": "[ \\t\\f]"
  },
  "states": {
    "default": [
      {
        "pattern": "^(#{1,6})${whiteSpace}+(.*)",
        "styles": [1, "keyword", 2, "class"]
      },
      {
        "pattern": "^(```)${whiteSpace}*(java)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockJava"
      },
      {
        "pattern": "^(```)${whiteSpace}*(javascript|js)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockJavaScript"
      },
      {
        "pattern": "^(```)${whiteSpace}*(typescript|ts)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockTypeScript"
      },
      {
        "pattern": "^(```)${whiteSpace}*(python|py)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockPython"
      },
      {
        "pattern": "^(```)${whiteSpace}*(c)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockC"
      },
      {
        "pattern": "^(```)${whiteSpace}*(cpp|c\\+\\+)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockCpp"
      },
      {
        "pattern": "^(```)${whiteSpace}*(csharp|cs|c#)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockCSharp"
      },
      {
        "pattern": "^(```)${whiteSpace}*(go|golang)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockGo"
      },
      {
        "pattern": "^(```)${whiteSpace}*(kotlin|kt)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockKotlin"
      },
      {
        "pattern": "^(```)${whiteSpace}*(rust|rs)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockRust"
      },
      {
        "pattern": "^(```)${whiteSpace}*(swift)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockSwift"
      },
      {
        "pattern": "^(```)${whiteSpace}*(dart)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockDart"
      },
      {
        "pattern": "^(```)${whiteSpace}*(lua)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockLua"
      },
      {
        "pattern": "^(```)${whiteSpace}*(shell|sh|bash|zsh)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockShell"
      },
      {
        "pattern": "^(```)${whiteSpace}*(sql)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockSql"
      },
      {
        "pattern": "^(```)${whiteSpace}*(html|htm)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockHtml"
      },
      {
        "pattern": "^(```)${whiteSpace}*(xml)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockXml"
      },
      {
        "pattern": "^(```)${whiteSpace}*(json)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockJson"
      },
      {
        "pattern": "^(```)${whiteSpace}*(yaml|yml)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockYaml"
      },
      {
        "pattern": "^(```)${whiteSpace}*(toml)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockToml"
      },
      {
        "pattern": "^(```)${whiteSpace}*(groovy)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockGroovy"
      },
      {
        "pattern": "^(```)${whiteSpace}*(t|tiecode)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockTiecode"
      },
      {
        "pattern": "^(```)${whiteSpace}*([a-zA-Z0-9_+-]*)${whiteSpace}*$",
        "styles": [1, "punctuation", 2, "annotation"],
        "state": "codeBlockPlain"
      },
      {
        "pattern": "(`)([^`]+)(`)",
        "styles": [1, "punctuation", 2, "string", 3, "punctuation"]
      },
      {
        "pattern": "^(>)${whiteSpace}*(.*)",
        "styles": [1, "keyword", 2, "comment"]
      },
      {
        "pattern": "^(\\*\\*\\*|---|___)${whiteSpace}*$",
        "style": "keyword"
      },
      {
        "pattern": "^(${whiteSpace}*)([-*+])${whiteSpace}+(\\[)([ xX])(\\])",
        "styles": [2, "punctuation", 3, "punctuation", 4, "keyword", 5, "punctuation"]
      },
      {
        "pattern": "^(${whiteSpace}*)([-*+])${whiteSpace}+",
        "styles": [2, "punctuation"]
      },
      {
        "pattern": "^(${whiteSpace}*)([0-9]+)(\\.)${whiteSpace}+",
        "styles": [2, "number", 3, "punctuation"]
      },
      {
        "pattern": "<!--.*?-->",
        "style": "comment"
      },
      {
        "pattern": "<!--",
        "style": "comment",
        "state": "htmlComment"
      },
      {
        "pattern": "(</?)([a-zA-Z][a-zA-Z0-9]*)([^>]*)(>)",
        "styles": [1, "punctuation", 2, "annotation", 4, "punctuation"]
      },
      {
        "pattern": "^(\\[)([^\\]]+)(\\]:)${whiteSpace}+(.+)",
        "styles": [1, "punctuation", 2, "variable", 3, "punctuation", 4, "string"]
      },
      {
        "pattern": "(!)?(\\[)([^\\]]*)(\\])(\\()([^)]*)(\\))",
        "styles": [1, "keyword", 2, "punctuation", 3, "annotation", 4, "punctuation", 5, "punctuation", 6, "string", 7, "punctuation"]
      },
      {
        "pattern": "(\\[\\^)([^\\]]+)(\\])",
        "styles": [1, "punctuation", 2, "variable", 3, "punctuation"]
      },
      {
        "pattern": "(\\[)([^\\]]*)(\\])(\\[)([^\\]]*)(\\])",
        "styles": [1, "punctuation", 2, "annotation", 3, "punctuation", 4, "punctuation", 5, "variable", 6, "punctuation"]
      },
      {
        "pattern": "\\\\[\\\\`*_{}\\[\\]()#+.!|~>-]",
        "style": "string"
      },
      {
        "pattern": "(\\*\\*|__)(.*?)(\\*\\*|__)",
        "styles": [1, "punctuation", 2, "keyword", 3, "punctuation"]
      },
      {
        "pattern": "(\\*|_)(.*?)(\\*|_)",
        "styles": [1, "punctuation", 2, "annotation", 3, "punctuation"]
      },
      {
        "pattern": "(~~)(.*?)(~~)",
        "styles": [1, "punctuation", 2, "comment", 3, "punctuation"]
      },
      {
        "pattern": "https?://[^\\s<>)\\]]+",
        "style": "string"
      },
      {
        "pattern": "^\\|.*\\|${whiteSpace}*$",
        "style": "variable",
        "state": "table"
      }
    ],
    "table": [
      {
        "pattern": "^(\\|[\\s:]*[-]+[\\s:]*)+\\|${whiteSpace}*$",
        "style": "punctuation"
      },
      {
        "pattern": "\\|",
        "style": "punctuation"
      },
      {
        "pattern": "(\\*\\*|__)(.*?)(\\*\\*|__)",
        "styles": [1, "punctuation", 2, "keyword", 3, "punctuation"]
      },
      {
        "pattern": "(\\*|_)(.*?)(\\*|_)",
        "styles": [1, "punctuation", 2, "annotation", 3, "punctuation"]
      },
      {
        "pattern": "(~~)(.*?)(~~)",
        "styles": [1, "punctuation", 2, "comment", 3, "punctuation"]
      },
      {
        "pattern": "(`)([^`]+)(`)",
        "styles": [1, "punctuation", 2, "string", 3, "punctuation"]
      },
      {
        "pattern": "^(?!\\|)",
        "style": "variable",
        "state": "default"
      },
      {
        "onLineEndState": "table"
      }
    ],
    "htmlComment": [
      {
        "pattern": "-->",
        "style": "comment",
        "state": "default"
      },
      {
        "pattern": "${any}",
        "style": "comment"
      }
    ],
    "codeBlockJava": [
      {
        "pattern": "^(```)${whiteSpace}*$",
        "styles": [1, "punctuation"],
        "state": "default"
      },
      {
        "importSyntax": "java"
      }
    ],
